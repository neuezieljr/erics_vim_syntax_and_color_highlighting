\begin{table}[h!]
\caption{Description of Factorial}
\begin{tabularx}{\textwidth}{ |X|X|X| }
  \hline
    \textbf{expression} & \textbf{mnemonic} & \textbf{value} \\
  \hline
    0! & 1 & 1\\
  \hline
    1! & 1 & 1\\
  \hline 
    2!  & 1*2 & 2 \\
  \hline
    3!  & 1*2*3 & 6 \\
  \hline
    6!  & 1*2*3*4*5*6 & 720 \\
  \hline
\end{tabularx}
\end{table}


